        <li class="form-line" data-type="control_button" id="id_13">
          <div style="text-align:center" data-align="center">
            <!--order sum v-->


            <b colspan="2" style="text-align:left;margin-top:20px;display:table"></b>
            <b colspan="2" style="text-align:left;margin-top:20px;display:table"> Order Sum</b>
            <b id="ordersum" colspan="2" style="text-align:left;margin-top:20px;display:table"  > </b>

           <!--****************************-->

            <button id="input_13" type="button" onclick="clickHand()"
              class="form-submit-button submit-button jf-form-buttons jsTest-submitField" data-component="button"
              data-content="">
              Submit Order
            </button>
            <script type="text/javascript">
              function clickHand() {

                var firstName = document.getElementById("first_2");
                var lastName = document.getElementById("last_2");
                var email = document.getElementById("input_3");
                var prePhone = document.getElementById("input_5_area");
                var midPhone = document.getElementById("input_5_phone");
                var date = document.getElementById("endDateModal");
                var ccFirstName = document.getElementById("input_17_cc_firstName");
                var ccLastName = document.getElementById("input_17_cc_lastName");
                var ccNumber = document.getElementById("input_17_cc_number");
                var ccSec = document.getElementById("input_17_cc_ccv");
                var ccExp = document.getElementById("input_17_cc_exp");
                var ccPostal = document.getElementById("input_17_cc_postal");
                var startDate = document.getElementById("startDateModal");
                var uid = document.getElementById("uidModal");
                const ordersum = document.getElementById("ordersum");
                var billTotalC = parseInt(ordersum.innerHTML);
               
                console.log(billTotalC);
                //***********CHECKS************//

                //Email
                if (ccNumber.value.length != 16) {
                  alert("Please fill the correct credit card number");
                  return;
                }

                //Phone
                if (prePhone.value.length != 3) {
                  alert("Please fill the correct phone area number");
                  return;
                }
                if (midPhone.value.length != 7) {
                  alert("Please fill the correct Phone number");
                  return;
                }

                //Postal
                if (ccPostal.value.length != 6) {
                  alert("Please fill the correct postal 6 numbers");
                  return;
                }

                //ExpoCard
                if (ccExp.value.length == 4) {
                  var m = ccExp.value[0] + ccExp.value[1];
                  var y = ccExp.value[2] + ccExp.value[3];
                  m = parseInt(m);
                  y = parseInt(y);
                  if (m > 12 || y < 20) {
                    alert("Please fill the correct year");
                    return;
                  }
                }


                const endd = document.getElementById("start");
                const minn = document.getElementById("minDateModal");
                var date1 = new Date(minn.value);
                var date2 = new Date(endd.value);
                var diff = (date2 - date1) / 1000;
                if (diff.value < 0) {
                  alert("Please enter date above the minimal date required");
                  return;
                }







                /* console.log(firstName.value+' ' +lastName.value);
                 console.log(email.value);
                 console.log(prePhone.value + midPhone.value);
                 console.log(date.value);
                 console.log(ccFirstName.value +ccLastName.value);
                 console.log(ccNumber.value);
                 console.log(ccSec.value);
                 console.log(ccExp.value);
                 console.log(ccPostal.value);*/
                var phoneNumber = prePhone.value + midPhone.value;
                fetch("/requestOrder", {
                  method: "POST",
                  headers: {
                    Accept: "application/json",
                    "Content-Type": "application/json",
                    "CSRF-Token": Cookies.get("XSRF-TOKEN"),
                  },
                  body: JSON.stringify({
                    email: email.value,
                    billTotal: billTotalC,
                    endDate: date.value,
                    firstName: firstName.value,
                    lastName: lastName.value,
                    phoneNumber: phoneNumber,
                    startDate: startDate.value,
                    unitid: uid.value,
                    ccFirstName: ccFirstName.value,
                    ccLastName: ccLastName.value,
                    ccNumber: ccNumber.value,
                    ccSec: ccSec.value,
                    ccExp: ccExp.value,
                    ccPostal: ccPostal.value
                  }),
                })

                  .then(response => response.json())
                  .then(function (resJ) {
                    console.log(resJ.data);

                  }).catch(function (error) {
                    console.log('data error');
                  });
              }
            </script>
          </div>
        </li>
    </div>
    </li>

