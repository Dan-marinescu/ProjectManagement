        <!--sum overall-->
        <script>
          function calcSum() {
            const startdate = document.getElementById("startDateModal");
            const endd = document.getElementById("start");
            const sum = document.getElementById("priceModal");
            var sumint = sum.value;
            var date1 = new Date(startdate.value);
            var date2 = new Date(endd.value);
            var diff = Math.abs(date2 - date1) / 1000;
            var days = Math.floor(diff / 86400);

            var temp = parseInt(sumint) * (days / 30);
            const ordersum = document.getElementById("ordersum");
            ordersum.innerHTML=parseInt(temp);
            console.log(ordersum.value);
          }

        </script>




